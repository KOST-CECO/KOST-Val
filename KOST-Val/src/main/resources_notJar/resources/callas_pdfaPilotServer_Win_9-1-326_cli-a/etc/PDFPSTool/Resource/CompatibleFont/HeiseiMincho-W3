/HeiseiMincho-W3 dup

/CIDInit /ProcSet resourcestatus
   { pop pop /CIDInit /ProcSet findresource true }
   {
   /AdobeTypeComposer /ProcSet resourcestatus
      { pop pop /AdobeTypeComposer /ProcSet findresource true }
      { false }
   ifelse
   }
ifelse

   {
   begin
   /HeiseiMin-W3 /CIDFont resourcestatus
      { pop pop [/HeiseiMin-W3-90pv-RKSJ-H /HeiseiMincho-W3-PropRoman] }
      { [/Ryumin-Light-90pv-RKSJ-H /HeiseiMincho-W3-PropRoman] }
   ifelse

   beginrearrangedfont
   1 usefont
   2 beginbfrange
      <00> <80> <00>
      <fd> <ff> <fd>
   endbfrange
   endrearrangedfont
   end
   }
   { pop pop /findfont errordict /invalidfont get exec stop }
ifelse

/CompatibleFont defineresource pop

%%EndResource
%%EOF
