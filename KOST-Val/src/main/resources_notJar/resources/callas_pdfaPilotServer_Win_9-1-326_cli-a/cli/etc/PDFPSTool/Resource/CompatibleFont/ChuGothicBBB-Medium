/ChuGothicBBB-Medium dup

/CIDInit /ProcSet resourcestatus
{ pop pop /CIDInit /ProcSet findresource true
}
{ /AdobeTypeComposer /ProcSet resourcestatus
  { pop pop /AdobeTypeComposer /ProcSet findresource true
  }
  { false 
  }
  ifelse
}
ifelse

{ begin
    /GothicBBB-Medium /CIDFont resourcestatus
    { pop pop
      [/GothicBBB-Medium-90pv-RKSJ-H /ChuGothicBBB-Medium-PropRoman]
    }
    { [/HeiseiKakuGo-W5-90pv-RKSJ-H /CGBM-PropRoman]
    } 
	ifelse

    beginrearrangedfont
    1 usefont
    2 beginbfrange
    <00> <80> <00>
    <fd> <ff> <fd>
    endbfrange
    endrearrangedfont
  end	
} 
{ pop pop /findfont errordict /invalidfont get exec stop
}
ifelse

/CompatibleFont defineresource pop

%%EndResource
