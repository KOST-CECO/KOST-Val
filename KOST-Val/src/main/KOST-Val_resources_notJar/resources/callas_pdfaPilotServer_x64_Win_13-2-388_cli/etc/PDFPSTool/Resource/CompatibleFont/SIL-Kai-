currentglobal
true setglobal
/SIL-Kai- /CIDFont findresource
dup length dict copy
begin
  currentdict /FID known
    { currentdict /FID undef }
  if
  /FontInfo FontInfo dup length dict copy
  dup /OrigCMap /GBpc-EUC-H put
  dup /OrigFontName /SIL-Kai- put
  def
  currentdict
end
/SIL-Kai-YX1 exch /CIDFont defineresource pop
setglobal

/SIL-Kai- /SIL-Kai-YX1--GBpc-EUC-H findfont
dup length dict copy
begin
currentdict
/FID undef
currentdict
end
definefont
pop

%%EndResource
%%EOF
