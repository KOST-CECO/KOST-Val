/HYGungSo-Bold dup

/CIDInit /ProcSet resourcestatus
{ pop pop /CIDInit /ProcSet findresource true
}
{ /AdobeTypeComposer /ProcSet resourcestatus
  { pop pop /AdobeTypeComposer /ProcSet findresource true
  }
  { false
  }
  ifelse
}
ifelse

{ begin	
    [/HYGungSo-Bold-KSCpc-EUC-H /HYGungSo-Bold-KSCpc-EUC-H /HYFix] 
    beginrearrangedfont
    1 usefont
    2 beginbfrange
    <15> <15> <A2D2>
    <82> <82> <FE>
    endbfrange
    2 usefont
    6 beginbfrange
    <11> <14> <3D>
    <18> <1A> <42>
    <3C> <3E> <3A>
    <60> <60> <20>
    <80> <81> <45>
    <84> <84> <40>
    endbfrange
    endrearrangedfont  
  end	
} 
{ pop pop /findfont errordict /invalidfont get exec stop
}
ifelse

/CompatibleFont defineresource pop

%%EndResource
%%EOF
