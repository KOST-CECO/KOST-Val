/HYKHeadLine-Bold dup

/CIDInit /ProcSet resourcestatus
{ pop pop /CIDInit /ProcSet findresource true
}
{ /AdobeTypeComposer /ProcSet resourcestatus
  { pop pop /AdobeTypeComposer /ProcSet findresource true
  }
  { false
  }
  ifelse
}
ifelse

{ begin	
    [/HYKHeadLine-Bold-KSCpc-EUC-H /HYKHeadLine-Bold-KSCpc-EUC-H /HYFix] 
    beginrearrangedfont
    1 usefont
    3 beginbfrange
    <13> <13> <81>
    <15> <15> <A2D2>
    <82> <82> <FE>
    endbfrange
    2 usefont
    9 beginbfrange
    <11> <12> <31>
    <14> <14> <33>
    <18> <1A> <34>
    <2D> <2D> <2E>
    <3C> <3C> <2F>
    <3E> <3E> <30>
    <60> <60> <20>
    <80> <81> <37>
    <84> <84> <33>
    endbfrange
    endrearrangedfont  
  end	
} 
{ pop pop /findfont errordict /invalidfont get exec stop
}
ifelse

/CompatibleFont defineresource pop

%%EndResource
