currentglobal
true setglobal
/JCfg /CIDFont findresource
dup length dict copy
begin
  currentdict /FID known
    { currentdict /FID undef }
  if
  /FontInfo FontInfo dup length dict copy
  dup /OrigCMap /KSCpc-EUC-H put
  dup /OrigFontName /JCfg put
  def
  currentdict
end
/JCfgYX1 exch /CIDFont defineresource pop
setglobal

/JCfg /JCfgYX1--KSCpc-EUC-H findfont
dup length dict copy
begin
currentdict
/FID undef
currentdict
end
definefont
pop

%%EndResource
%%EOF
