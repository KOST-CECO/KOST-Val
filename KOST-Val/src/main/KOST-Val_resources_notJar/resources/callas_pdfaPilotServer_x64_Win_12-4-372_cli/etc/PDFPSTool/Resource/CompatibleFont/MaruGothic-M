currentglobal
true setglobal
/MaruGothic-M /CIDFont findresource
dup length dict copy
begin
  currentdict /FID known
    { currentdict /FID undef }
  if
  /FontInfo FontInfo dup length dict copy
  dup /OrigCMap /90pv-RKSJ-H put
  dup /OrigFontName /MaruGothic-M put
  def
  currentdict
end
/MaruGothic-MYX1 exch /CIDFont defineresource pop
setglobal

/MaruGothic-M /MaruGothic-MYX1--90pv-RKSJ-H findfont
dup length dict copy
begin
currentdict
/FID undef
currentdict
end
definefont
pop

%%EndResource
%%EOF
