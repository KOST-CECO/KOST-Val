currentglobal
true setglobal
/Osaka /CIDFont findresource
dup length dict copy
begin
  currentdict /FID known
    { currentdict /FID undef }
  if
  /FontInfo FontInfo dup length dict copy
  dup /OrigCMap /90pv-RKSJ-H put
  dup /OrigFontName /Osaka put
  def
  currentdict
end
/OsakaYX1 exch /CIDFont defineresource pop
setglobal

/Osaka dup

 /CIDInit /ProcSet findresource begin
   [/OsakaYX1--90pv-RKSJ-H /OsakaYX1--90pv-RKSJ-H]
   beginrearrangedfont
   1 usefont
   2 beginbfrange
      <00> <80> <00>
      <fd> <ff> <fd>
   endbfrange
   endrearrangedfont
 end

/CompatibleFont defineresource pop

%%EndResource
